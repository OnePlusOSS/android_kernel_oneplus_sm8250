&pm8009_gpios{
        cam_sensor_pmi_gpio {
                cam_sensor_rear_2_ana_active: cam_sensor_rear_2_ana_active {
                        pins = "gpio1";
                        function = "normal";
                        power-source = <0>;
                        output-low;
                        input-disable;
                };

