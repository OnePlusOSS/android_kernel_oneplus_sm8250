&tlmm {
    cam_sensor_rear_0_clock_active: cam_sensor_rear_0_clock_active {
        /* MCLK0 */
        mux {
	    pins = "gpio94";
            function = "cam_mclk";
        };

        config {
               pins = "gpio94";
               bias-disable; /* No PULL */
               drive-strength = <2>; /* 2 MA */
        };
    };

    cam_sensor_rear_0_clock_suspend: cam_sensor_rear_0_clock_suspend {
	/* MCLK0 */
	mux {
            pins = "gpio94";
            function = "cam_mclk";
        };

	config {
	       pins = "gpio94";
	       bias-pull-down; /* PULL DOWN */
               drive-strength = <2>; /* 2 MA */
	};
    };

